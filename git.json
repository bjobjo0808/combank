{
    "name":"GIT",
    "rows": [
        {
            "type": "title",
            "name": "show"
        }, 
        {
            "type": "recipe",
            "name": "コミットの詳細を見る",
            "command": "git show [@1]",
            "description": "プッシュ",
            "input1": "コミットID"
        }, 
        {
            "type": "title",
            "name": "log"
        }, 
        {
            "type": "recipe",
            "name": "ログの一覧",
            "command": "git log --oneline",
            "description": "ログの一覧"
        }, 
        {
            "type": "recipe",
            "name": "ログの一覧文字列検索",
            "command": "git log --oneline --grep='[@1]'",
            "description": "ログの一覧文字列検索",
            "input1": "文字列"
        }, 
        {
            "type": "title",
            "name": "stash"
        }, 
        {
            "type": "recipe",
            "name": "一覧",
            "command": "git stash list",
            "description": "stash一覧"
        }, 
        {
            "type": "recipe",
            "name": "退避",
            "command": "git stash save [@1]",
            "description": "stash退避",
            "input1": "コメント"
        }, 
        {
            "type": "recipe",
            "name": "戻し",
            "command": "git stash pop stash@{[@1]}",
            "description": "stash戻し",
            "input1": "番号"
        }, 
        {
            "type": "title",
            "name": "commit"
        }, 
        {
            "type": "recipe",
            "name": "コミット",
            "command": "git commit -v",
            "description": "コミット"
        }, 
        {
            "type": "title",
            "name": "push"
        }, 
        {
            "type": "recipe",
            "name": "プッシュ",
            "command": "git push origin [@1]",
            "description": "プッシュ",
            "input1": "ブランチ名"
        }, 
        {
            "type": "title",
            "name": "checkout"
        }, 
        {
            "type": "recipe",
            "name": "チェックアウト",
            "command": "git checkout -b [@1]",
            "description": "チェックアウト(ブランチなければ作る)",
            "input1": "ブランチ名"
        },
        {
            "type": "title",
            "name": "reset"
        }, 
        {
            "type": "recipe",
            "name": "リセット(ハード)",
            "command": "git reset --hard HEAD",
            "description": "リセット(ハード)"
        }, 
        {
            "type": "title",
            "name": "tag"
        }, 
        {
            "type": "recipe",
            "name": "ブランチにタグを打つ",
            "command": "git tag -a '[@1]' -m '[@2]'",
            "description": "ブランチにタグを打つ",
            "input1": "タグ名",
            "input2": "コメント"
        }, 
        {
            "type": "recipe",
            "name": "タグをブランチに反映する",
            "command": "git push origin --tag",
            "description": "タグをブランチに反映する"
        }, 
        {
            "type": "title",
            "name": "diff"
        }, 
        {
            "type": "recipe",
            "name": "次回コミット時の差分を確認する",
            "command": "git diff --cached",
            "description": "次回コミット時の差分を確認する"
        }, 
        {
            "type": "recipe",
            "name": "コミットID間の差分を取る",
            "command": "git diff [@1] [@2]",
            "description": "コミット間の差分を取る",
            "input1": "対象コミットID",
            "input2": "対象コミットID"
        }, 
        {
            "type": "recipe",
            "name": "コミットID間の差分を取る(ファイル名のみ)",
            "command": "git diff [@1] [@2] --name-only",
            "description": "コミットID間の差分を取る(ファイル名のみ)",
            "input1": "対象コミットID",
            "input2": "対象コミットID"
        }, 
        {
            "type": "recipe",
            "name": "タグ間の差分を取る",
            "command": "git diff --diff-filter=AMCRD [@1] [@2]",
            "description": "タグ間の差分を取る",
            "input1": "タグ名",
            "input2": "タグ名"
        }
    ]
}