{
    "name":"GIT",
    "rows": [
        {
            "type": "title",
            "name": "git"
        }, 
        {
            "type": "recipe",
            "name": "コミットの詳細を見る",
            "command": "git show [@1]",
            "description": "プッシュ",
            "input1": "コミットID"
        }, 
        {
            "type": "recipe",
            "name": "コミット",
            "command": "git commit -v",
            "description": "コミット"
        }, 
        {
            "type": "recipe",
            "name": "プッシュ",
            "command": "git push origin [@1]",
            "description": "プッシュ",
            "input1": "ブランチ名"
        }, 
        {
            "type": "recipe",
            "name": "チェックアウト",
            "command": "git checkout -b [@1]",
            "description": "チェックアウト(ブランチなければ作る)",
            "input1": "ブランチ名"
        },
        {
            "type": "recipe",
            "name": "リセット(ハード)",
            "command": "git reset --hard HEAD",
            "description": "リセット(ハード)"
        }, 
        {
            "type": "recipe",
            "name": "ブランチにタグを打つ",
            "command": "git tag -a '[@1]' -m '[@2]'",
            "description": "ブランチにタグを打つ",
            "input1": "タグ名",
            "input2": "コメント"
        }, 
        {
            "type": "recipe",
            "name": "タグをブランチに反映する",
            "command": "git push origin --tag",
            "description": "タグをブランチに反映する"
        }, 
        {
            "type": "recipe",
            "name": "次回コミット時の差分を確認する",
            "command": "git diff --cached",
            "description": "次回コミット時の差分を確認する"
        }, 
        {
            "type": "recipe",
            "name": "コミットID間の差分を取る",
            "command": "git diff [@1] [@2]",
            "description": "コミット間の差分を取る",
            "input1": "対象コミットID",
            "input2": "対象コミットID"
        }, 
        {
            "type": "recipe",
            "name": "コミットID間の差分を取る(ファイル名のみ)",
            "command": "git diff [@1] [@2] --name-only",
            "description": "コミットID間の差分を取る(ファイル名のみ)",
            "input1": "対象コミットID",
            "input2": "対象コミットID"
        }, 
        {
            "type": "recipe",
            "name": "タグ間の差分を取る",
            "command": "git diff --diff-filter=AMCRD [@1] [@2]",
            "description": "タグ間の差分を取る",
            "input1": "タグ名",
            "input2": "タグ名"
        }
    ]
}